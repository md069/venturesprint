{
  "version": 2,
  "buildCommand": "turbo build",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/app/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "app.venturesprint.io"
        }
      ],
      "destination": "apps/app/:path*"
    },
    {
      "source": "/:path*",
      "destination": "apps/web/:path*"
    }
  ]
}
