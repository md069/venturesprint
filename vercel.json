{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "buildCommand": "turbo build",
    "builds": [
      {
        "src": "apps/web/package.json",
        "use": "@vercel/next",
        "config": {
          "path": "apps/web"
        }
      },
      {
        "src": "apps/app/package.json",
        "use": "@vercel/next",
        "config": {
          "path": "apps/app"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/app/api/:path*",
        "destination": "apps/app/api/:path*"
      },
      {
        "source": "/app/:path*",
        "destination": "apps/app/:path*"
      },
      {
        "source": "/:path*",
        "destination": "apps/web/:path*"
      }
    ]
}
  